-- Terminos para testear parseo
-- PCF0
5
(fun (x : Nat) -> pred x) 3
x
ifz 0 then 2 else 3
fix (suma: Nat -> Nat -> Nat) (m: Nat) -> fun (n:Nat) -> ifz n then m else succ (suma m (pred n))


{-
-- PCF1 (terminos)
-- 1.1
let x : Nat = 1 in succ x

-- 1.2
let f (x : Nat) : Nat = succ x in f 2

-- 1.3

-- 1.4
let suc2 (x y: Nat) (z:Nat) : Nat = suc (suc x) in suc2 1 0 0


